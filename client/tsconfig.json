{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["node"],
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"],
      "#build/ui": [
        "./node_modules/@nuxt/ui/.nuxt/ui"
      ],
      "#imports": [".nuxt/imports.d.ts"]
    }
  },
  "include": ["src", "vite.config.ts, src/**/*.ts\", \"src/**/*.tsx\", \"src/**/*.vue\", \"auto-imports.d.ts\", \"components.d.ts"]
}
